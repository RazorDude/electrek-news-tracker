{
	"name": "Electrek News Tracker",
	"version": "1.0",
	"description": "Electrek News Tracker",
	"permissions": [
		"notifications",
		"cookies",
		"https://electrek.co/"
	],
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},
	"browser_action": {
		"default_icon": "electrekIcon16.png",
		"default_popup": "popup.html",
		"icons": {"16": "electrekIcon16.png"}
	},
	"icons": {"48": "electrekIcon48.png"},
	"manifest_version": 2
}